<div class="page-header">
  <h1>
    Feelings
    <a href="/feelings/new" class="btn btn-lg btn-success">New Feeling</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @feelings.each do |feeling| %>
      <tr>
        <td><%= feeling.name %></td>
        <td>
          <a href="/feelings/<%= feeling.id %>" class="btn btn-primary">Show</a>
          <a href="/feelings/<%= feeling.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_feeling/<%= feeling.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
