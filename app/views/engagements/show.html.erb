<div class="page-header">
  <h3>Engagement #<%= @engagement.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @engagement.user_id %></dd>

      <dt>Artwork</dt>
      <dd><%= @engagement.artwork_id %></dd>

      <dt>Notes</dt>
      <dd><%= @engagement.notes %></dd>
      
      <dt>Feeling</dt>
      <dd>
        <%= @engagement.feeling_id %> 
      </dd>
      
      <dt>Date</dt>
      <dd><%= @engagement.created_at %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/engagements" class="btn btn-primary">
        Back
      </a>
      <a href="/engagements/<%= @engagement.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_engagement/<%= @engagement.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
